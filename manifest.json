{
  "name": "Chrome Dotfiles",
  "description": "Customize your favorite apps with gists",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "permissions": [
    "https://gist.github.com/",
    "https://github.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "http://*/*"],
      "js": ["injector.js"]
    }
  ]
}