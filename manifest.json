{
  "name": "Chrome Dotfiles",
  "description": "Customize your favorite apps with gists",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "storage",
    "https://gist.github.com/",
    "https://github.com/"
  ],
  "background": {
    "scripts": ["build/build.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup/index.html"
  }
}
